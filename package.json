  },
  "homepage": "https://github.com/modernserf/redeclare#readme",
  "devDependencies": {
    "babel-eslint": "^7.1.0",
    "buble": "^0.11.5",
    "eslint": "^2.13.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-react": "^6.7.1",
    "eslint-plugin-standard": "^1.3.2",
    "faucet": "0.0.1",
